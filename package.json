{
  "name": "sfdx-plugin-auth-url",
  "description": "sfdx plugin for importing/exporting orgs using a Sfdx Auth Url",
  "version": "0.0.0-development",
  "author": "Matthias Rolke @amtrack",
  "bugs": "https://github.com/amtrack/sfdx-plugin-auth-url/issues",
  "bin": {
    "sfdx-plugin-auth-url": "bin/run"
  },
  "dependencies": {
    "@salesforce/command": "5.3.8",
    "tmp": "0.2.1",
    "tslib": "2.5.0"
  },
  "devDependencies": {
    "@oclif/test": "2.3.20",
    "@salesforce/ts-sinon": "1.4.6",
    "@types/mocha": "10.0.1",
    "@types/tmp": "0.2.3",
    "chai": "4.3.7",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "oclif": "3.9.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=14.16"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/amtrack/sfdx-plugin-auth-url",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "auth-url": {
        "description": "Commands for importing/exporting orgs using a Sfdx Auth Url."
      }
    },
    "additionalHelpFlags": [
      "-h"
    ]
  },
  "repository": "amtrack/sfdx-plugin-auth-url",
  "scripts": {
    "build": "rm -rf lib && tsc -p . && oclif manifest",
    "prepack": "yarn build",
    "prepare": "yarn build",
    "test": "nyc --reporter=lcov --reporter=text mocha --require ts-node/register \"test/**/*.test.ts\""
  }
}
